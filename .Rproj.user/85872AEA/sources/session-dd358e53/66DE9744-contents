verbose = TRUE

unique(ta$AREA)
unique(tb$AREA)
unique(tc$AREA)


ta <- headers.conversion(ta,"TA")
tb <- headers.conversion(tb,"TB")
tc <- headers.conversion(tc,"TC")

################ NON MODIFICARE #############


RoMEcc(TA=ta,
     TB=tb,
     TC=tc,
     TE=te,
     TL=tl,
     wd=wd,
     suffix=suffix,
     verbose=verbose,
     Stratification=Stratification,
     Ref_list=TM_list,
     DataTargetSpecies=DataTargetSpecies,
     Maturity=Maturity_parameters,
