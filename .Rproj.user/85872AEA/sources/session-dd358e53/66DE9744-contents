tc <- read.csv("D:/OneDrive - Coispa Tecnologia & Ricerca S.C.A.R.L/_CONDIVISA con COSMIDANO_/RoME_2022/CHECK_WZ_CN/TC.csv", sep=";")


te <- NA # read.table(file=paste(wd, "\\TE.csv",sep=""), sep=";", header=T)

tl <- NA # read.table(file=paste(wd, "\\TL.csv",sep=""), sep=";", header=T)



Stratification= RoME::stratification_scheme # oppure = read.table(file=paste(wd,"\\Tables\\Stratification_Scheme.csv",sep=""), sep=";", header=T)
